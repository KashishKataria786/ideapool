

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model idea{
  id          Int      @id @default(autoincrement())
  title       String
  description String         
  upvotes     Int     @default(0)
  email  String
  createdAt   DateTime @default(now())
}